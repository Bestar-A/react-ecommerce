{
  "name": "react-ecommerce",
  "version": "1.0.0",
  "description": "ecommerce application built with the MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node multivendor-app_backend/server.js",
    "server": "nodemon multivendor-app_backend/server.js",
